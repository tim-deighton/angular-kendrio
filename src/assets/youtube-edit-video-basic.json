{
	"title": "Edit Video",
	"description": "This form allows you to edit your video",
	"type": "object",
	"required": [
		"title"
	],

	"properties": {
		"title": {
			"title": "Title",
			"description": "",
			"type": "money",
			"minLength": 2,
			"maxLength": 100
		},
		"country": {
			"type": "typeahead",
			"title": "tags",
			"items": {
				"type": "string",
				"default": "bazinga"
			}
		},
		"description": {
			"type": "textarea",
			"title": "Bio",
			"minLength": 2,
			"maxLength": 5000
		}
	}
}