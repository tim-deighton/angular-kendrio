{
	"title": "A registration form",
	"description": "A simple form example.",
	"type": "object",
	"required": [
		"firstName",
		"isdn",
		"claim"
	],
	"properties": {

		"title": {
			"title": "Artist",
			"description": "Please select your Artist",
			"type": "string",
			"enum": ["David Boxie",
				"The Clash",
				"Sex Pistols",
				"The Fall"
			]
		},


		"country": {
			"type": "typeahead",
			"title": "tags",
			"items": {
				"type": "string",
				"default": "bazinga"
			}
		},

		"description": {
			"type": "textarea",
			"title": "Bio"
		},





	}
}