{
	"title": "Edit Video",
	"description": "This form allows you to edit your video",
	"type": "object",
	"required": [
		"title"
	],
	"properties": {

		"title": {
			"title": "Title",
			"description": "",
			"type": "string",
       "minLength": 2,
        "maxLength": 100 
		},


		"country": {
			"type": "typeahead",
			"title": "tags",
			"items": {
				"type": "string",
				"default": "bazinga"
			}
		},

		"description": {
			"type": "textarea",
			"title": "Bio",
       "minLength": 2,
        "maxLength": 5000 
		},


		"visibility": {
			"title": "Artist",
			"description": "Please select your Artist",
			"type": "string",
			"enum": [
        "Public",
				"Private",
				"Unlisted"
			]
		}




	}
}