{
  "title": "Edit Video",
  "description": "This form allows you to edit your video",
  "type": "object",
  "required": [
    "title"
  ],

 
  "properties": {
    "videoUrl": {
      "title": "https://www.youtube.com/embed/YA9N4nsAxZo",
      "type": "videoviewer",
    },
     		"pay": {
			"type": "money",
			"title": "Income",
			"minLength": 10,
			"description": "(Less is more)"
		},
    "title": {
      "title": "Title",
      "description": "",
      "type": "string",
      "minLength": 2,
      "maxLength": 100
    },
    "country": {
      "type": "typeahead",
      "title": "tags",
      "items": {
        "type": "string",
        "default": "bazinga"
      }
    },
    "description": {
      "type": "textarea",
      "title": "Bio",
      "minLength": 2,
      "maxLength": 5000
    },
 
  }
}